// Constructor Calling 
class Test
{
	void m1()
		{
				this(10);
				System.out.println("m1 method");
		}

	void m2(int a)
		{
				this(10,20);
				System.out.println("m2 method");
		}
	void m3(int a, int b)
		{
				System.out.println("m3 method");
		}
	} 
	public static void main(String[] args) 
	{
		Test t = new Test();
	}
}
